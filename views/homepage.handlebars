{{#each posts as |post|}}
<div class="card">
    <div class="create-form">
       
        <button type="submit" > <a href="/post/{{post.id}}">View Post</a></button>
        <h5> <a href="/post/{{post.id}}">{{post.title}}</a></h5>
    </div>
    <p>Written by: {{post.user.name}}</p>
</div>



{{/each}}

 